{
  "name": "aws-lambda",
  "scripts": {
    "build": "node esbuild.js",
    "register": "ts-node ./src/register.ts",
    "package": "sam package --resolve-s3",
    "deploy": "sam deploy --resolve-s3 --stack-name ESPNFantasyFootballPoller --capabilities CAPABILITY_IAM",
    "invoke": "npm run build && sam local invoke --event ./sample/event.json"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.102",
    "@types/node": "18.6.2",
    "aws-sdk": "2.1199.0",
    "dotenv-cli": "6.0.0",
    "esbuild": "0.15.5",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "espnff": "github:tarsillon1/espnff"
  }
}
